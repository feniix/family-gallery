    "@types/node-cache": "^4.2.5",
    "@types/react": "^19.1.7",
    "cypress": "^14.4.1",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "eslint-plugin-react-hooks": "^5.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
