    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "commander": "^14.0.0",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "exifr": "^7.1.3",
    "lucide-react": "^0.513.0",
    "next": "15.3.3",
    "next-themes": "^0.4.6",
    "node-cache": "^5.1.2",
    "postcss": "^8.4.49",
    "react": "^19.0.0",
